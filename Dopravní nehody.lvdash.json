    {
      "name": "4b8aa10c",
      "displayName": "Poměr zranění a úmrtí",
      "queryLines": [
        "SELECT Year(Datum_nehody) as Rok_nehody, \r\n",
        "       Month(Datum_nehody) as Mesic_nehody,\r\n",
        "       count(*) as nehody_celkem,\r\n",
        "       count(*)-count_if(p13a=0) as nehody_s_umrtim,\r\n",
        "       CAST((((count(*)-count_if(p13a=0))/count(*))*100) as DECIMAL(5,2)) as procento_smrtelnych_nehod,\r\n",
        "       count_if(p13a=0 and p13b=0 and p13c=0) as nehody_bez_nasledku_na_zdravi,\r\n",
        "       CAST((count_if(p13a=0 and p13b=0 and p13c=0)/count(*))*100 as DECIMAL(5,2)) as procento_nehod_bez_nasledku_na_zdravi,\r\n",
        "       count_if(p13a=0 and (p13b>0 or p13c>0)) as nehody_zraneni_bez_umrti,\r\n",
        "       sum(p13a) as umrti,\r\n",
        "       sum(p13b) as tezke_zraneni,\r\n",
        "       sum(p13c) as lehke_zraneni,\r\n",
        "       sum(p14) as hmotna_skoda \r\n",
        "FROM workspace.default.nehody\r\n",
        "GROUP BY Year(Datum_nehody), Month(Datum_nehody)\r\n",
        "ORDER BY Year(Datum_nehody), Month(Datum_nehody)"
      ]
    },
    {
      "name": "b52999d3",
      "displayName": "Dny v týdnu",
      "queryLines": [
        "SELECT dayname(Datum_nehody) as Den,\r\n",
        "       year(Datum_nehody) as Rok_nehody,\r\n",
        "       month(Datum_nehody) as Mesic_nehody,\r\n",
        "       count(*) as Pocet_nehod,\r\n",
        "       (count(*)/SUM(COUNT(*)) OVER()) as Procentualni_zastoupeni\r\n",
        "FROM nehody\r\n",
        "GROUP BY Den, Rok_nehody, Mesic_nehody\r\n",
        "ORDER BY pocet_nehod DESC;"
      ]
    },
    {
      "name": "4873dcab",
      "displayName": "Frekvence",
      "queryLines": [
        "SELECT p1,\r\n",
        "       Datum_nehody,\r\n",
        "       MONTH(Datum_nehody) as Mesic,\r\n",
        "       year(Datum_nehody) as Rok,\r\n",
        "       dayname(Datum_nehody) as Den\r\n",
        "FROM nehody"
      ]
    },
    {
      "name": "604deb20",
      "displayName": "Druhy nehody",
      "queryLines": [
        "WITH filtered_p6 AS (\r\n",
        "SELECT * FROM documentace_cz WHERE formular_DN = 'p6'\r\n",
        ")\r\n",
        "SELECT YEAR(n.Datum_nehody) as Rok_nehody, MONTH(n.Datum_nehody) as Mesic_nehody, f.popis as Druh_nehody,  COUNT(*) as pocet FROM nehody as n\r\n",
        "LEFT JOIN filtered_p6 as f ON n.p6 = f.polozka\r\n",
        "GROUP BY Rok_nehody, Mesic_nehody, Druh_nehody;\r\n"
      ]
    }
  ],
