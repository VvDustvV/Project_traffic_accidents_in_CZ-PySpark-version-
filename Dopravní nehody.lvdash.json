    {
      "name": "4b8aa10c",
      "displayName": "Poměr zranění a úmrtí",
      "queryLines": [
        "SELECT Year(Datum_nehody) as Rok_nehody, \r\n",
        "       Month(Datum_nehody) as Mesic_nehody,\r\n",
        "       count(*) as nehody_celkem,\r\n",
        "       count(*)-count_if(p13a=0) as nehody_s_umrtim,\r\n",
        "       CAST((((count(*)-count_if(p13a=0))/count(*))*100) as DECIMAL(5,2)) as procento_smrtelnych_nehod,\r\n",
        "       count_if(p13a=0 and p13b=0 and p13c=0) as nehody_bez_nasledku_na_zdravi,\r\n",
        "       CAST((count_if(p13a=0 and p13b=0 and p13c=0)/count(*))*100 as DECIMAL(5,2)) as procento_nehod_bez_nasledku_na_zdravi,\r\n",
        "       count_if(p13a=0 and (p13b>0 or p13c>0)) as nehody_zraneni_bez_umrti,\r\n",
        "       sum(p13a) as umrti,\r\n",
        "       sum(p13b) as tezke_zraneni,\r\n",
        "       sum(p13c) as lehke_zraneni,\r\n",
        "       sum(p14) as hmotna_skoda \r\n",
        "FROM workspace.default.nehody\r\n",
        "GROUP BY Year(Datum_nehody), Month(Datum_nehody)\r\n",
        "ORDER BY Year(Datum_nehody), Month(Datum_nehody)"
      ]
    },
    {
      "name": "b52999d3",
      "displayName": "Dny v týdnu",
      "queryLines": [
        "SELECT dayname(Datum_nehody) as Den,\r\n",
        "       year(Datum_nehody) as Rok_nehody,\r\n",
        "       month(Datum_nehody) as Mesic_nehody,\r\n",
        "       count(*) as Pocet_nehod,\r\n",
        "       (count(*)/SUM(COUNT(*)) OVER()) as Procentualni_zastoupeni\r\n",
        "FROM nehody\r\n",
        "GROUP BY Den, Rok_nehody, Mesic_nehody\r\n",
        "ORDER BY pocet_nehod DESC;"
      ]
    },
    {
      "name": "4873dcab",
      "displayName": "Frekvence",
      "queryLines": [
        "SELECT p1,\r\n",
        "       Datum_nehody,\r\n",
        "       MONTH(Datum_nehody) as Mesic,\r\n",
        "       year(Datum_nehody) as Rok,\r\n",
        "       dayname(Datum_nehody) as Den\r\n",
        "FROM nehody"
      ]
    },
    {
      "name": "3cc37a1f",
      "displayName": "Druh nehody",
      "queryLines": [
        "WITH filtered_p6 AS (\r\n",
        "  SELECT * FROM dokumentace_cz WHERE formular_DN = 'p6'\r\n",
        "),\r\n",
        "filtered_p7 AS (\r\n",
        "  SELECT * FROM dokumentace_cz WHERE formular_DN = 'p7'\r\n",
        ")\r\n",
        "\r\n",
        "SELECT YEAR(n.Datum_nehody) as rok_nehody,\r\n",
        "       MONTH(n.Datum_nehody) as mesic_nehody,\r\n",
        "       fp6.popis as druh_nehody,\r\n",
        "       CONCAT(fp6.popis, ' ', fp6.blizsi_definice) as detail_druhu,      \r\n",
        "       fp7.popis as druh_srazky_vozidel,\r\n",
        "       COUNT(*) as pocet \r\n",
        "FROM nehody as n\r\n",
        "LEFT JOIN filtered_p6 as fp6 ON n.p6 = fp6.polozka\r\n",
        "LEFT JOIN filtered_p7 as fp7 ON n.p7 = fp7.polozka\r\n",
        "GROUP BY rok_nehody, \r\n",
        "         mesic_nehody, \r\n",
        "         druh_nehody, \r\n",
        "         detail_druhu, \r\n",
        "         druh_srazky_vozidel;"
      ]
    },
    {
      "name": "c513850b",
      "displayName": "Příčiny nehody",
      "queryLines": [
        "WITH filtered_p6 AS (\r\n",
        "  SELECT * FROM dokumentace_cz WHERE formular_DN = 'p6'\r\n",
        "),\r\n",
        "filtered_p8 AS (\r\n",
        "  SELECT * FROM dokumentace_cz WHERE formular_DN = 'p8'\r\n",
        "),\r\n",
        "filtered_p8a AS (\r\n",
        "  SELECT * FROM dokumentace_cz WHERE formular_DN = 'p8a'\r\n",
        "),\r\n",
        "filtered_p10 AS (\r\n",
        "  SELECT * FROM dokumentace_cz WHERE formular_DN = 'p10'\r\n",
        "),\r\n",
        "filtered_p11 AS (\r\n",
        "  SELECT * FROM dokumentace_cz WHERE formular_DN = 'p11'\r\n",
        "),\r\n",
        "filtered_p11a AS (\r\n",
        "  SELECT * FROM dokumentace_cz WHERE formular_DN = 'p11a'\r\n",
        "),\r\n",
        "filtered_p12 AS (\r\n",
        "  SELECT * FROM dokumentace_cz WHERE formular_DN = 'p12'\r\n",
        "),\r\n",
        "filtered_p15 AS (\r\n",
        "  SELECT * FROM dokumentace_cz WHERE formular_DN = 'p15'\r\n",
        "),\r\n",
        "filtered_p16 AS (\r\n",
        "  SELECT * FROM dokumentace_cz WHERE formular_DN = 'p16'\r\n",
        "),\r\n",
        "filtered_p18 AS (\r\n",
        "  SELECT * FROM dokumentace_cz WHERE formular_DN = 'p18'\r\n",
        "),\r\n",
        "filtered_p19 AS (\r\n",
        "  SELECT * FROM dokumentace_cz WHERE formular_DN = 'p19'\r\n",
        "),\r\n",
        "filtered_p29 AS (\r\n",
        "  SELECT * FROM dokumentace_cz WHERE formular_DN = 'p29'\r\n",
        "),\r\n",
        "filtered_p29a AS (\r\n",
        "  SELECT * FROM dokumentace_cz WHERE formular_DN = 'p29a'\r\n",
        "),\r\n",
        "filtered_p30 AS (\r\n",
        "  SELECT * FROM dokumentace_cz WHERE formular_DN = 'p30'\r\n",
        "),\r\n",
        "filtered_p30a AS (\r\n",
        "  SELECT * FROM dokumentace_cz WHERE formular_DN = 'p30a'\r\n",
        "),\r\n",
        "filtered_p30b AS (\r\n",
        "  SELECT * FROM dokumentace_cz WHERE formular_DN = 'p30b'\r\n",
        "),\r\n",
        "filtered_p31 AS (\r\n",
        "  SELECT * FROM dokumentace_cz WHERE formular_DN = 'p31' \r\n",
        ")\r\n",
        "\r\n",
        "\r\n",
        "SELECT fp6.popis as druh_nehody,\r\n",
        "       fp8.popis as druh_pevne_prekazky,\r\n",
        "       fp8a.popis as druh_zvirete,\r\n",
        "       fp10.popis as zavineni_nehody,\r\n",
        "       fp11.popis as alkohol_u_vinika,\r\n",
        "       fp11a.popis as drogy_u_vinika,\r\n",
        "       fp12.popis as hlavni_pricina,\r\n",
        "       fp15.popis as druh_vozovky,\r\n",
        "       fp16.popis as stav_povrchu_vozovky,\r\n",
        "       fp18.popis as stav_vozovky,\r\n",
        "       fp19.popis as viditelnost,\r\n",
        "       fp29.popis as kategorie_chodce,\r\n",
        "       fp29a.popis as reflexni_prvky,\r\n",
        "       fp30.popis as stav_chodce,\r\n",
        "       fp30a.popis as alkohol_u_chodce,\r\n",
        "       fp30b.popis as drogy_u_chodce,\r\n",
        "       fp31.popis as chovani_chodce,\r\n",
        "       count(*) as pocet,\r\n",
        "       (count(*)/SUM(COUNT(*)) OVER()) as procento\r\n",
        "FROM nehody as n\r\n",
        "LEFT JOIN chodci as ch ON n.p1 = ch.p1\r\n",
        "LEFT JOIN filtered_p6 as fp6 ON n.p6 = fp6.polozka\r\n",
        "LEFT JOIN filtered_p8 as fp8 ON n.p8 = fp8.polozka\r\n",
        "LEFT JOIN filtered_p8a as fp8a ON n.p8a = fp8a.polozka\r\n",
        "LEFT JOIN filtered_p10 as fp10 ON n.p10 = fp10.polozka\r\n",
        "LEFT JOIN filtered_p11 as fp11 ON n.p11 = fp11.polozka\r\n",
        "LEFT JOIN filtered_p11a as fp11a ON n.p11a = fp11a.polozka\r\n",
        "LEFT JOIN filtered_p12 as fp12 ON n.p12 = fp12.polozka\r\n",
        "LEFT JOIN filtered_p15 as fp15 ON n.p15 = fp15.polozka\r\n",
        "LEFT JOIN filtered_p16 as fp16 ON n.p16 = fp16.polozka\r\n",
        "LEFT JOIN filtered_p18 as fp18 ON n.p18 = fp18.polozka\r\n",
        "LEFT JOIN filtered_p19 as fp19 ON n.p19 = fp19.polozka\r\n",
        "LEFT JOIN filtered_p29 as fp29 ON ch.p29 = fp29.polozka\r\n",
        "LEFT JOIN filtered_p29a as fp29a ON ch.p29a = fp29a.polozka\r\n",
        "LEFT JOIN filtered_p30 as fp30 ON ch.p30 = fp30.polozka\r\n",
        "LEFT JOIN filtered_p30a as fp30a ON ch.p30a = fp30a.polozka\r\n",
        "LEFT JOIN filtered_p30b as fp30b ON ch.p30b = fp30b.polozka\r\n",
        "LEFT JOIN filtered_p31 as fp31 ON ch.p31 = fp31.polozka\r\n",
        "GROUP BY druh_nehody, druh_pevne_prekazky, druh_zvirete, zavineni_nehody, alkohol_u_vinika, drogy_u_vinika, hlavni_pricina, druh_vozovky, stav_povrchu_vozovky, \r\n",
        "         stav_vozovky, viditelnost, kategorie_chodce, reflexni_prvky, stav_chodce, alkohol_u_chodce, drogy_u_chodce, chovani_chodce"
      ]
    }
